					"builder": "@angular-devkit/build-angular:extract-i18n"
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "projects/reports/tsconfig.spec.json",
						"assets": [
							{
								"glob": "**/*",
								"input": "projects/reports/public",
								"output": "/assets"
