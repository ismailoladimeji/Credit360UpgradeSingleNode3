						],
						"stylePreprocessorOptions": {
							"includePaths": [
								"node_modules"
							]
						},
						"scripts": []
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kB",
									"maximumError": "1MB"
								},
								{
									"type": "anyComponentStyle",
