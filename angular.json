						],
						"styles": [
							"projects/reports/src/styles.css",
							"projects/reports/src/assets/scss/styles.scss"
						],
						"stylePreprocessorOptions": {
							"includePaths": [
								"node_modules"
							]
						},
						"scripts": []
					}
				}
			}
		},
		"boostrap-template": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				},
				"@schematics/angular:directive": {
					"standalone": false
				},
				"@schematics/angular:pipe": {
					"standalone": false
				}
			},
			"root": "projects/boostrap-template",
			"sourceRoot": "projects/boostrap-template/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:application",
					"options": {
						"allowedCommonJsDependencies": [
							"apexcharts"
						],
						"outputPath": "dist/boostrap-template",
						"index": "projects/boostrap-template/src/index.html",
						"browser": "projects/boostrap-template/src/main.ts",
						"polyfills": [
							"src/polyfills.ts"
						],
						"tsConfig": "projects/boostrap-template/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							{
								"glob": "**/*",
								"input": "projects/boostrap-template/public",
								"output": "/assets"
							}
						],
						"styles": [
							"projects/boostrap-template/src/styles.css",
							"projects/boostrap-template/src/styles.scss"
						],
						"stylePreprocessorOptions": {
							"includePaths": [
								"node_modules"
							]
						},
						"scripts": [
							"projects/boostrap-template/src/assets/vendors/scripts/core.js",
							"projects/boostrap-template/src/assets/vendors/scripts/script.min.js",
							"projects/boostrap-template/src/assets/vendors/scripts/process.js",
							"projects/boostrap-template/src/assets/vendors/scripts/layout-settings.js"
						]
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "1.5mb",
									"maximumError": "1.5mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all",
							"optimization": {
								"scripts": true,
								"styles": {
									"minify": true,
									"inlineCritical": false
								}
							}
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "boostrap-template:build:production"
						},
						"development": {
							"buildTarget": "boostrap-template:build:development"
						}
					},
					"defaultConfiguration": "development"
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"buildTarget": "boostrap-template:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "projects/boostrap-template/tsconfig.spec.json",
						"assets": [
							{
								"glob": "**/*",
								"input": "projects/boostrap-template/public",
								"output": "/assets"
							}
						],
						"styles": [
							"projects/boostrap-template/src/styles.css",
							"projects/boostrap-template/src/styles.scss"
						],
						"stylePreprocessorOptions": {
							"includePaths": [
								"node_modules"
							]
						},
						"scripts": []
					}
				},
				"lint": {
					"builder": "@angular-eslint/builder:lint",
					"options": {
						"lintFilePatterns": [
							"src/**/*.ts",
							"src/**/*.html"
						]
					}
				}
			}
		},
